  <DMS_Apparel_MissilePod.comps.CompApparelReloadable.cooldownGerund>冷卻中</DMS_Apparel_MissilePod.comps.CompApparelReloadable.cooldownGerund>
  <!-- EN: launch missile -->
  <DMS_Apparel_MissilePod.verbs.launch_missile.label>發射導彈</DMS_Apparel_MissilePod.verbs.launch_missile.label>
  
